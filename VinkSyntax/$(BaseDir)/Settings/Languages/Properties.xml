<?xml version="1.0" encoding="utf-8"?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<languages>
		<language name="properties">
			<lexer name="properties" style-bits="7" />
			<file-extensions>properties,ini,yaml</file-extensions>
			<!-- COLORING_START -->
			<editor-style caret-fore="0xffffff" caretline-back="0x282828" selection-fore="0xffffff" selection-back="0x002040" margin-fore="0x000000" margin-back="0x000000" marker-fore="0x000000" marker-back="0x808080" colorize-marker-back="true" />
			<use-styles>
				<style name="default" fore="0xffffff" back="0x000000" size="10" font="Consolas" bold="true" />
				<style name="comment" fore="0x808080" />
				<style name="section" fore="0xffffff" back="0x00a0ff" eolfilled="true" />
				<style name="assignment" />
				<style name="defval" />
				<style name="key" fore="0xFFC66A" />
				<style name="gdefault" fore="0x333333" />
				<style name="linenumber" fore="0x6897BB" />
				<style name="bracelight" back="0x5c5c5c" bold="true" />
				<style name="bracebad" bold="true" />
				<style name="controlchar" fore="0xffffff" />
				<style name="indentguide" fore="0x808080" />
				<style name="lastpredefined" fore="0x666666" />
			</use-styles>
			<!-- COLORING_END -->
		</language>
	</languages>
</Scintilla>